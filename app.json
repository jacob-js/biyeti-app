{
  "expo": {
    "name": "bookit_app",
    "slug": "bookit_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Autoriser l'application à accéder à vos photos"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Autoriser l'application à accéder à vos photos",
          "savePhotosPermission": "Autoriser l'application à enregistrer des photos dans votre galerie",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jacobdev.bookit",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.jacobdev.bookit",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
